{
  "compilerOptions": {
    "declaration": false,
    "esModuleInterop": true,
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "node",
    "lib": [
      "dom",
      "esnext"
    ],
    "newLine": "lf",
    "outDir": "./dist",
    "preserveConstEnums": true,
    "removeComments": true,
    "strict": true,
    "resolveJsonModule": true
  },
  "include": [
    "*.ts"
  ],
  "exclude": [
    "dist",
    "node_modules",
    "**/*.test.ts"
  ]
}
