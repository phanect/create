{
  "name": "@phanect/create",
  "version": "2024.3.2",
  "description": "Personal Project Template for @phanect",
  "repository": {
    "url": "phanect/create",
    "type": "git"
  },
  "author": "Jumpei Ogawa (https://phanective.org)",
  "license": "MIT",
  "bin": "./bin/cli.js",
  "scripts": {
    "gen": "node ./bin/cli.js ./gen",
    "test": "ava",
    "snapshot": "ava --update-snapshots",
    "check": "npm run check:biome && npm run check:ejs",
    "check:biome": "biome check",
    "check:ejs": "ejslint ./template/*",
    "fix": "biome check --write",
    "release": "npm publish --access public --non-interactive"
  },
  "dependencies": {
    "sao": "^1.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@phanect/configs": "latest",
    "ava": "^6.0.1",
    "ejs-lint": "latest"
  },
  "ava": {
    "files": [
      "test/**/*",
      "!template/test"
    ]
  }
}
